{
    "train": {
        "loss": "ElasticArcFace", 
        "backbone": "mobilenet",
        "use_pretrained": false,
        "pretrained_backbone_path": "./weights/mobilenet.pth",
        "root_dir": "./data/datav2",
        "num_worker": 8,
        "freeze_model": true,
        "batch_size_train": 64,
        "batch_size_val": 64,
        "n_epochs": 30,
        "use_sam_optim": false,
        "sam_optim":
        {
            "momentum": 0.9,
            "rho": 0.05,
            "adaptive": false
        },
        "scheduler":
        {
            "name": "StepLR",
            "step_size": 10,
            "gamma": 0.5,
            "verbose": true
        },
        "use_lamb_optim": false,
        "learning_rate": 5e-4,
        "prefix": "e_arcface",
        "save_model": false,
        "verbose": 1
    },
    "test": {
        "trainset_path": "./data/grooo112",
        "testset_path": "./data/asian_masked",
        "backbone": "irse50",
        "pretrained_model_path": "./logs/arcface_irse50.pth",
        "num_worker": 8,
        "batch_size": 32
    }
}